pipeline {
  agent any
  stages {
    stage('Build'){
      steps {
       echo "build step"
       sleep 10
      }
    }
     stage('Test'){
      steps {
       echo "test stage"
       sleep 10
      }
  }
    stage('Deploy'){
      steps {
       echo "deploy stage"
       sleep 10
      }
  }
     stage('Docker'){
      steps {
       echo "docker image"
       sleep 10
      }
     }
     stage('Maven'){
      steps {
       echo "maven job"
       sleep 10
      }
     }
  }

}
     
